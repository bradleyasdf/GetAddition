function setIndex(){
    if (document.all){
    document.body.style.behavior='url(#default#homepage)';
    document.body.setHomePage("http://"+idxDom+"/");
  }else if (window.sidebar){
        if(window.netscape){
	       try{
	          netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
	       }catch (e){
	                    alert( "该操作被浏览器拒绝，如果想启用该功能，请在地址栏内输入 about:config,然后将项 signed.applets.codebase_principal_support 值改为true" );
	       }
        }
    var prefs = Components.classes['@mozilla.org/preferences-service;1'].getService(Components. interfaces.nsIPrefBranch);
    prefs.setCharPref('browser.startup.homepage',"http://"+idxDom+"/");
    }
}
function showapp(){
	var displayValue = jQuery(".BodyTC").css('display');
	if(displayValue=='none'){
		jQuery(".BodyTC").show();
		jQuery(".alertbg").show();
	}else{
		jQuery(".BodyTC").hide();
		jQuery(".alertbg").hide();
	}
}

function dyjcapp(){
	var displayValue = jQuery(".BodyDYJC").css('display');
	if(displayValue=='none'){
		jQuery(".BodyDYJC").show();
		jQuery(".alertbg").show();
	}else{
		jQuery(".BodyDYJC").hide();
		jQuery(".alertbg").hide();
	}
}
