var readurl=null;
(function(global,$){
	var factory  = function($){
		return {
            init: function () {
                this.loginCheck();
            },
            loginCheck: function(){
                var url = "http://ss.chaoxing.com/passport2islogin?callback=?";
                var _f = this;
                $.ajax({
                    cache: false,
                    dataType: 'jsonp',
                    get: 'post',
                    url: url,
                    success: function (data) {
                        if (data.login) {
                            _f.loginState = true;
                            readrl=" http://i.mooc.chaoxing.com/space/index";
                        } else {
                            _f.loginState = false;
                            readrl=" http://passport2.chaoxing.com/login";
                        }
                    }
                });
            },
            subscribeWin: function (cataid,key,content) {
                if (!this.loginState){
                    if(window.confirm("收藏需要登录，是否现在登录？")){
                        location.href = "http://passport2.chaoxing.com/login?refer=" + encodeURIComponent(location.href);
                    } else {
                        return;
                    }
                }
                collectToSpace(cataid,key,content);
            }
		};
	};
	global.collect = factory($);
}
)(window,jQuery);



